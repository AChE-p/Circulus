<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BBoxBoard.Equipment"
        xmlns:ed="http://schemas.microsoft.com/expression/2010/drawing" x:Class="BBoxBoard.Equipment.Ohmmeter"
        mc:Ignorable="d"
        Title="Ohmmeter" Height="300" Width="300" ResizeMode="NoResize">
    <Grid>
        <Grid.Background>
            <LinearGradientBrush EndPoint="0.139,0.916" StartPoint="0.139,0.961">
                <GradientStop Color="#FFCAF4E0" Offset="0"/>
                <GradientStop Color="White" Offset="1"/>
            </LinearGradientBrush>
        </Grid.Background>
        <TextBlock x:Name="textBlock" Margin="25.548,70.837,38.068,96.29" TextWrapping="Wrap" FontSize="36"><Run Text="{Binding SimpleData}"/><LineBreak/><Run/></TextBlock>
        <Canvas x:Name="ohmCanvas" Margin="10"/>

    </Grid>
</Window>
